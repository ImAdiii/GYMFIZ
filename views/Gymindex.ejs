}
.text-accent-300 {
  color: var(--clr-accent-300);
}
.text-white {
  color: var(--clr-white);
}

.text-shadow-dark {
  text-shadow: var(--text-shadow-dark);
}
.text-shadow-light {
  text-shadow: var(--text-shadow-light);
}

/* typography */

.fw-bold {
